<form class='comment-form'>
  <div class="detailBox">
      <div class="titleBox">
        <label>Write a Review!</label>
          <button type="button" class="close" aria-hidden="true">&times;</button>
      </div>
      <!-- <div class="commentBox">
          <p class="taskDescription">Lorem Ipsum is simply dummy text of the printing and typesetting industry.</p>
      </div> -->
      <div class="actionBox">
          <ul class="commentList">
              <li>
            <% listing.comments().each(function(comment) { %>
                <div class="commenterImage">
                  <!-- <img src="http://lorempixel.com/50/50/people/6" /> -->
                </div>
                <div class="commentText">
                    <p class=""> <%= comment.get('content') %> </p> <span class="date sub-text">on March 5th, 2014</span>
                </div>
            <% }) %>
              </li>
          </ul>
          <form class="form-inline comment-form" role="form">
              <div class="form-group">
                  <input class="form-control" type="text" name='content' placeholder="Your comments" />
              </div>
              <div class="form-group">
                  <button class="btn btn-default comment-listing">Add Review</button>
              </div>
          </form>
      </div>
  </div>
</form>
